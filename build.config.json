{
	"appId": "com.blossomos.installer",
	"productName": "blossomos-installer",
	"directories": {
		"output": "dist"
	},
	"files": [
		"src/electron.cjs",
		"src/preload.cjs",
		{
			"from": "build",
			"to": "",
			"filter": ["**/*"]
		}
	],
	"linux": {
		"target": [
			{
				"target": "pacman",
				"arch": ["x64"]
			}
		],
		"category": "Utility"
	}
}
